{
    "name": "Mr.Jimaku",
    "version": "1.0",
    "description": "Show your caption on the YouTube",
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "run_at": "document_end",
            "css": ["style.css"],
            "js": ["contentScript.js"]
        }
    ],
    "permissions": [
    ],
    "icons": {
        "32": "images/mrjimaku_icon32.png",
        "48": "images/mrjimaku_icon48.png",
        "128":"images/mrjimaku_icon128.png"
    },
    "manifest_version": 2
}
